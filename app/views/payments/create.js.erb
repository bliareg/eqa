<% if @payments.present? %>
  $('#content').html("<%= j render 'users/payment_history' %>");
  window.history.pushState('dashboard', '', "<%= payments_path %>");
  $('a.link_user_profile_menu').removeClass('active')
  $('a.link_user_profile_menu').last().addClass('active')
  $('.load-block').hide();
<% end %>
